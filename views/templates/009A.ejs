    <!-- GALLERY -->
    <div class="gallery">
      <div class="container">
        <div class="box-gallery" id="box-gallery">
          <div class="collum-gallery">
            <img src="../templates/009A/img/gallery-1.png" alt="" data-aos="fade" data-aos-duration="600" class="img-1">
            <img src="../templates/009A/img/gallery-2.png" alt="" data-aos="fade" data-aos-duration="600" class="img-2">
            <img src="../templates/009A/img/gallery-3.png" alt="" data-aos="fade" data-aos-duration="600" class="img-3">
            <img src="../templates/009A/img/gallery-4.png" alt="" data-aos="fade" data-aos-duration="600" class="img-4">
          </div>
          <div class="collum-gallery">
            <img src="../templates/009A/img/gallery-1.png" alt="" data-aos="fade" data-aos-duration="600" class="img-5">
            <img src="../templates/009A/img/gallery-4.png" alt="" data-aos="fade" data-aos-duration="600" class="img-6">
